<%= render 'static/header_unal_render' %>
<div class="page-header">
  <h1>General information</h1>
</div>


<div class="col-xs-12">
  <table class="table table-striped table-condensed">
      <thead>
        <tr>
          <th>Number</th>
          <th>Name</th>
          <th>Options</th>
        </tr>
      </thead>
      <tbody>
        <% @buildings.each do |b| %>
        <tr>
          <th><%= b.number %></th>
          <th><%= b.name %></th>
          <th><%= link_to 'Details', show_path(b.id) %></th>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>


<%= render 'paginate' %>

<%= render 'static/footer_unal_render' %>
